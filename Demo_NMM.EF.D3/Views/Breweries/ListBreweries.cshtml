@model IEnumerable<Demo_NMM.EF.D2.Models.Brewery>

@{
    ViewBag.Title = "ListBreweries";
}

<h2>Microbreweries in Northern Michigan</h2>

<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Edit", new { id = item.ID }) <br /> 
                @Html.DisplayFor(modelItem => item.City) <br />
                @Html.DisplayFor(modelItem => item.Phone) <br />
                <a href="http://@item.URL">@item.URL</a> <br />
            </td>
        </tr>
    }

</table>
ut